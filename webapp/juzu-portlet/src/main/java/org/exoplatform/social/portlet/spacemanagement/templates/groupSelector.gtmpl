#{param name=allGroups/}

<div class="ClearFix">
  <div id="GroupNavigation" class="GroupNavigation"></div>
  <span id="navText" class="nav-text">...</span>
</div>
<div id="GroupSelector" class="GroupSelector ClearFix">
  <div id="LeftSelector" class="LeftSelector">
		<div id="UISelectGroup">
		  <ul>
		  <% if (allGroups.getSibilings().size() > 0) {
		      def allGroups = allGroups.getSibilings(); 
		      for (groupNode in allGroups) {
		        def key = groupNode.getId().replace("/", "");
                def label = groupNode.getLabel();
		    %>
		        <li id="<%= key %>" val="<%= label %>" title="<%= label %>">
		          <a href="#"><%= key %></a>
		          <ul id="">
		            <%
		              def children = groupNode.getChirldren();
		              for (child  in children) {
		                def keyChild = child.getId().replace("/", "");
		                def labelChild = child.getLabel();
		            %>
		                <li id="<%= keyChild %>" val="<%= labelChild %>" title="<%= labelChild %>"><a href="javascript:void(0);"><%= labelChild %></a></li>
		            <% } %>    
		          </ul>
		        </li>
		    <% } %>
		  <% } %>
		  </ul>
		</div>
		<p id="msg" class="alert-info"></p>
	</div>
	<div id="RightSelector" class="RightSelector">
	  <ul>
	    <span>...</span>
	  </ul>
	</div>
</div>
